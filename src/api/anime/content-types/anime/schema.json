{
  "kind": "collectionType",
  "collectionName": "animes",
  "info": {
    "singularName": "anime",
    "pluralName": "animes",
    "displayName": "anime",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seasons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::season.season"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 20
    }
  }
}
